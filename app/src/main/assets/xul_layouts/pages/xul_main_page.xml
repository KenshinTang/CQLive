                                <attr name="minimum-item">60</attr>
                                <attr name="cache-pages">10</attr>
                                <template id="@item-template">
                                    <area class="playbill-item">
                                        <action event="click" type="script/javascript">
                                            var play_url = this.getData("play_url");
                                            var live_id = this.getData("live_id");
                                            var name = this.getData("name");
                                            var play_status = this.getData("play_status");
                                            if (play_status == "1") {
                                                if (this.findItemById("playing_indicator").getAttr("img.0.visible") == "false") {
                                                    this.dispatchEvent("doPlayback", "usr_cmd", JSON.stringify({"name":name,"live_id":live_id,"play_url":play_url}));
                                                }
                                            } else if (play_status == "2") {
                                                logger.d("kenshin", " play live!!!");
                                            } else {
                                                logger.d("kenshin", " do reservation!!!");
                                            }
                                        </action>
                                        <action event="bindingFinished" type="script/javascript">
                                            var play_status = this.getData("play_status");
                                            var program_name = this.findItemById("program_name");
                                            var program_time = this.findItemById("program_time");
                                            var program_state = this.findItemById("program_state");
                                            if (play_status == "1") {
                                                program_name.setStyle("font-color", "FFFFFFFF");
                                                program_time.setStyle("font-color", "FFFFFFFF");
                                                program_state.setStyle("display", "block");
                                                program_state.setAttr("text", "回看");
                                            } else if (play_status == "2") {
                                                program_name.setStyle("font-color", "FFFFFFFF");
                                                program_time.setStyle("font-color", "FFFFFFFF");
                                                program_state.setStyle("display", "block");
                                                program_state.setAttr("text", "直播");
                                            } else {
                                                program_name.setStyle("font-color", "B3B3B3B3");
                                                program_time.setStyle("font-color", "B3B3B3B3");
                                                program_state.setStyle("display", "none");
                                            }
                                        </action>
                                        <attr name="width">match_parent</attr>
                                        <attr name="height">140</attr>
                                        <data name="play_status" binding="[=play_status]"/>
                                        <data name="play_url" binding="[=play_url]"/>
                                        <data name="name" binding="[=name]"/>
                                        <data name="live_id" binding="[=live_id]"/>
                                        <item id="playing_indicator" class="playing_indicator" type="image" desc="正在播放图标">
                                            <attr name="x">20</attr>
                                            <attr name="y">58</attr>
                                            <attr name="img.0.width">28</attr>
                                            <attr name="img.0.height">24</attr>
                                            <attr name="img.0">file:///.assets/images/playing.ani.zip</attr>
                                            <attr name="img.0.visible">false</attr>
